def asal(sayi,bolen=2):
    for i in range(1,sayi):
      if (sayi%bolen == 0):
        print(bolen)
        sayi /= bolen
        sayi = int(sayi)
        return asal(sayi, bolen)

      else:
        bolen += 1
    if(sayi==1):
        return 1
        
sayi = int(input("asalcarpani bulacagimiz sayiyi giriniz:"))
asal(sayi)

